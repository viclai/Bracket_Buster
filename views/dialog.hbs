                    <li class="game game-bottom">
                      <span class="bb-seed-real">{{team2.seed}}</span>
                      <span class="bb-seed">{{team2.seed}}</span>

                      <span class="bb-team-name-real">{{team2.name}}</span>
                      <span class="bb-team-name">{{team2.name}}</span>

                      <span class="bb-score">{{team2.score}}</span>
                      <input type="radio" name="round1-{{add @index 24}}"
                        class="bb-team-pick" value="{{team2.name}}" />
                    </li>

                    <li class="spacer">&nbsp;</li>
                  {{/each}}
                </ul>
              </td>
            </tr>
          </table>
        </td>
      {{/with}}
    </tr>
  </table>
{{/if}}
