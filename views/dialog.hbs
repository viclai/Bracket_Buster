{{!< layout}}

{{#contentFor 'pageStyles'}}
  <style>
    body {
      font-family: sans-serif;
      font-size: small;
      padding: 10px;
      line-height: 1.4em;
    }
  </style>
{{/contentFor}}

{{#contentFor 'pageScripts'}}
  <script src="/js/bracket-pick.js"></script>
{{/contentFor}}

<!--
  The dialog has a primary action, declared in the descriptor
  atlassian-connect.json
-->
<!-- It is implemented in the js/addon.js file imported in the layout -->

{{#if bracketData.state.unavailable}}
  {{!-- Bracket is either not ready yet or there is an error. --}}
  <div>The bracket is not available at this time.<div>
{{else}}
  <a href="" target="_blank">Open in New Tab</a>
  {{#if bracketData.state.set}}
    <input id="bb-set" hidden/>
