{{!< layout}}

{{#contentFor 'pageStyles'}}
<style>
  body {
    font-family: sans-serif;
    font-size: small;
    padding: 10px;
    line-height: 1.4em;
  }
</style>
{{/contentFor}}

<!-- The dialog has a primary action, declared in the descriptor atlassian-connect.json -->
<!-- It is implemented in the js/addon.js file imported in the layout -->

<a href="" target="_blank">Open in New Tab</a>
<table style="width: 100%; margin-left: auto; margin-right: auto;">
  <tr>
    <td style="width: 50%;">
      <table align="left">
        <tr class="half-bracket">
          <td class="region"> <!-- East -->
            <ul class="round" style="margin-top: 10px;">
              <li class="spacer">&nbsp;</li>

              {{#each bracketData.east.[1st Round]}}
                <li class="game game-top">
                  <span class="seed">{{team1.seed}}</span> {{team1.name}} <span class="score">{{team1.score}}</span>
                </li>
                <li class="game game-spacer-left">&nbsp;</li>
                <li class="game game-bottom">
                  <span class="seed">{{team2.seed}}</span> {{team2.name}} <span class="score">{{team2.score}}</span>
                </li>

                <li class="spacer">&nbsp;</li>
              {{/each}}
            </ul>

            <ul class="round">
