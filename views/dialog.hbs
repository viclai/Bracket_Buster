      {{#with bracketData.regions.region1}}
        <td style="width: 50%;">
          <table align="left">
            <tr class="half-bracket">
              <td class="region"> <!-- {{name}} -->
                <ul class="round" style="margin-top: 10px;">
                  <li class="spacer">&nbsp;</li>

                  {{#each [1st Round]}}
                    <li class="game game-top">
                      <span class="bb-seed-real">{{result.team1.seed}}</span>
                      <span class="bb-seed">{{prediction.team1.seed}}</span>

                      <span class="bb-team-name-real">
                        {{~result.team1.name~}}
                      </span>
                      <span class="bb-team-name">
                        {{~prediction.team1.name~}}
                      </span>

                      <span class="bb-score">{{{result.team1.score}}}</span>
                      <input type="radio" name="round1-{{add @index 0}}"
                        class="bb-team-pick" value="{{result.team1.name}}" />
                    </li>
                    <li class="game game-spacer-left">&nbsp;</li>
                    <li class="game game-bottom">
                      <span class="bb-seed-real">{{{result.team2.seed}}}</span>
                      <span class="bb-seed">{{{prediction.team2.seed}}}</span>

                      <span class="bb-team-name-real">
                        {{result.team2.name~}}
