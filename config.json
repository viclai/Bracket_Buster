{
    "development": {
        "localBaseUrl": "https://bb02ebec.ngrok.io",
        "usePublicKey": false,
        "watch": false,
        "port": 3000,
        "maxTokenAge": 86400,
        "store": {
            "adapter": "jugglingdb",
            "type": "sqlite3",
            "database": "store.db"
        }
    },
    "production": {
        "usePublicKey": false,
        "port": "$PORT",
        "localBaseUrl": "https://your-subdomain.herokuapp.com",
        "store": {
            "adapter": "redis",
            "url": "$REDIS_URL"
        },
        "whitelist": [
            "*.hipchat.com"
        ]
    },
    "product": "hipchat"
}
